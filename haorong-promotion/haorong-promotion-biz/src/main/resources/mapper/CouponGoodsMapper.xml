<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haorong.cloud.promotion.mapper.CouponGoodsMapper">
    <select id="selectByCouponId" resultType="com.haorong.cloud.promotion.api.entity.CouponGoods">
        SELECT coupon_goods.coupon_id,
               coupon_goods.spu_id
        FROM coupon_goods AS coupon_goods
        WHERE coupon_goods.`coupon_id` = #{couponId}
        AND coupon_goods.`del_flag`='0'
    </select>
</mapper>